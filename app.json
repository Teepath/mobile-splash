{
  "name": "mobile-flashcards",
  "displayName": "mobile-flashcards",
  "expo": {
    "name": "mobile-flashcards",
    "slug": "mobile-flashcards",
    "version": "1.0.0",
    "assetBundlePatterns": [
      "**/*"
    ],
 
    "infoPlist": {
      "NSRemindersUsageDescription": "This app uses the reminder to notify you about the quiz."
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./local/path/to/myNotificationIcon.png",
          "color": "#ffffff",
          "sounds": ["./local/path/to/mySound.wav", "./local/path/to/myOtherSound.wav"],
          "mode": "production"
        }
      ]
    ]
  }
}
