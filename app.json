{
  "name": "mobile-flashcards",
  "displayName": "mobile-flashcards",
  "expo": {
    "name": "mobile-flashcards",
    "slug": "mobile-flashcards",
    "version": "1.0.0",
    "assetBundlePatterns": [
      "**/*"
    ],
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./local/path/to/myNotificationIcon.png",
          "color": "#ffffff",
          "sounds": [
            "./local/path/to/mySound.wav",
            "./local/path/to/myOtherSound.wav"
          ],
          "mode": "production"
        }
      ]
    ],
    "android": {
      "package": "com.teepath.mobileflashcards"
    },
    "ios": {
      "bundleIdentifier": "com.teepath.mobileflashcards"
    }
  }
}
